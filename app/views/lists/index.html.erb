<%= render(partial:'layouts/navbar') %>
<% if @lists.any? %>
  <ul>
    <% @lists.each do |list| %>
      <li>
        <%= link_to list.name, list_path(list) %>
        <%= list.description %>
        <%= list.number_of_likes %>
      </li>
    <% end %>
  </ul>
  <% else %>
    <br/>
    <h5>You have no lists to choose from, go ahead and ADD one</h5> 
    <%= link_to 'Create your first Focus list', new_list_path %> 
<% end %>
